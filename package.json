{
  "name": "jordan-scrapes-amazon-for-products",
  "version": "1.0.0",
  "description": "Using puppeteer, we web scrape Amazon to find a suitable product to sell.",
  "main": "./dist/index.js",
  "scripts": {
    "build": "node node_modules/typescript/bin/tsc",
    "watch": "node node_modules/typescript/bin/tsc --watch",
    "prestart": "node node_modules/typescript/bin/tsc",
    "start": "node ./dist/index.js",
    "start:noInstall": "tsc && node ./dist/index.js",
    "start:ubuntu": "tsc && node ./dist/index.js ubuntu",
    "start:headless": "tsc && node ./dist/index.js headless",
    "massSearch": "tsc && node ./dist/massSearch.js",
    "massSearch:ubuntu": "tsc && node ./dist/massSearch.js ubuntu",
    "massSearch:headless": "tsc && node ./dist/massSearch.js headless"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aarmora/jordan-scrapes-amazon-for-products-to-sell.git"
  },
  "keywords": [
    "Javascript",
    "web",
    "scraping",
    "puppeteer",
    "amazon"
  ],
  "author": "Jordan Hansen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aarmora/jordan-scrapes-amazon-for-products-to-sell/issues"
  },
  "homepage": "https://github.com/aarmora/jordan-scrapes-amazon-for-products-to-sell#readme",
  "dependencies": {
    "puppeteer": "^1.11.0",
    "puppeteer-helpers": "^1.0.10",
    "database-helpers": "2.0.2",
    "webhook-discord": "3.0.3"
  },
  "devDependencies": {
    "@types/node": "^11.11.6",
    "@types/puppeteer": "^1.11.1",
    "tslint": "^5.14.0",
    "typescript": "^3.2.2"
  }
}
